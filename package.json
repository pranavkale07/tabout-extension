{
  "name": "Tabout for LeetCode",
  "version": "0.1.0",
  "description": "Tab out of brackets, quotes, etc in LeetCode's code editor",
  "scripts": {
    "prebuild": "node scripts/update-readme-version.js",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:watch": "webpack --mode=development --watch",
    "clean": "rm -rf dist/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "version": "node -e \"console.log(require('./package.json').version)\"",
    "zip": "cd dist && zip -r ../releases/tabout-extension-$(npm run version --silent).zip * -x '*.map'"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^30.1.2",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "terser-webpack-plugin": "^5.3.7",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "private": true
}
