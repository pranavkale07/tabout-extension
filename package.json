    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^30.1.2",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "terser-webpack-plugin": "^5.3.7",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "private": true
}
